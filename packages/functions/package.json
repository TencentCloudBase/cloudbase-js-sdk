{
  "name": "@cloudbase/functions",
  "version": "1.2.1",
  "description": "cloudbase javascript sdk functions componets",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "tsc": "rm -rf dist/ && tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "build": "npm run lint && npm run tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TencentCloudBase/cloudbase-js-sdk"
  },
  "keywords": [
    "cloudbase",
    "tcb",
    "javascript",
    "typescript",
    "web",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cloudbase/adapter-interface": "^0.4.0",
    "@cloudbase/types": "^1.1.0",
    "@cloudbase/utilities": "^1.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.7.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  }
}
